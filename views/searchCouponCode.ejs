<!DOCTYPE html>
<html lang="en">
<head>
    <style>

    </style>
</head>

<body>
  <div class="container-fluid">
    <div class="alert alert-secondary text-center">
      <h1>View coupon from selected coupon code</h1>
    </div>
    <ul>
      <% coupon.forEach(function(cp) { %>
        <table style= "float:left" border ="1">
          <tr>
          <td style="padding:15px">
            <Strong>Coupon: </Strong><%= cp.coupon_code %><br/>
            <Strong>Discount: </Strong><%= cp.discount %><br/>
          </td>
        </tr>
      </table>
        <% }); %>
    </ul>
    </div>
    <br/><br/><br/><br/><br/><br/><br/><br/><br/><br/>
    <button type="button" style="border-color:red" onclick="Back()">Back</button>
    <script>
      function Back()
      {
        location.href = "adminPage.html";
      }
    </script>
</body>
</html>
